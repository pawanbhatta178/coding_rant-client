 React.useContext(ModalContext);\n  const { challenge, challengeDispatch } = React.useContext(ChallengeContext);\n  const { editor, editorDispatch } = React.useContext(EditorContext);\n  const {\n    submission,\n    submissionDispatch,\n    submitting,\n    setSubmitting,\n  } = React.useContext(SubmissionContext);\n  const { activeQuestionId, setActiveQuestionId } = React.useContext(\n    QuestionContext\n  );\n\n  return (\n    <div {...props}>\n      <Button size=\"sm\" type=\"basic\" onClick={() => setActiveQuestionId(\"1\")}>\n        <Icon type=\"Previous\" />\n      </Button>\n      <Button\n        size=\"sm\"\n        type={\"neutral\"}\n        disabled={submitting}\n        onClick={() => {\n          if (!user) {\n            return modalDispatch({ type: \"SHOW_LOGIN\" });\n          }\n          setSubmitting(true);\n          challengeDispatch({ type: \"SHOW_RESULT\" });\n          compile({\n            source: editor.code,\n            lang: editor.chosenLang,\n            questionId: activeQuestionId,\n          }).then((res) => {\n            console.log(submission);\n            submissionDispatch({\n              type: \"ADD_SUBMISSION\",\n              payload: {\n                id: activeQuestionId,\n                submission: res,\n              },\n            });\n            setSubmitting(false);\n          });\n        }}\n      >\n        <div>Submit</div>\n      </Button>\n      <Button size=\"sm\" type=\"basic\" onClick={() => setActiveQuestionId(\"2\")}>\n        <Icon type=\"Next\" />\n      </Button>\n    </div>\n  );\n};\n\nexport default EditorAction;\n",["269","270"],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/QuestionNavBar.js",[],["271","272"],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/reducers/challengeReducer.js",[],["273","274"],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/Question.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/QuestionResult.js",[],"/Users/pawan