 => {\n  const editorRef = useRef();\n  const [isEditorReady, setIsEditorReady] = useState(false);\n  const { chosenLang, startingCodes } = useContext(EditorContext);\n\n  function handleEditorDidMount(_, editor) {\n    setIsEditorReady(true);\n    editorRef.current = editor;\n  }\n\n  return (\n    <div className=\"code-editor-height border  \">\n      <MonacoEditor\n        height=\"100%\"\n        language={availableLanguage[chosenLang] || \"javascript\"}\n        value={startingCodes[chosenLang]}\n        editorDidMount={handleEditorDidMount}\n      />\n    </div>\n  );\n};\nexport default Editor;\n","/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/SplitPane.js",["179"],"import React from \"react\";\n\nconst splitPaneContext = React.createContext();\n\nconst SplitPane = ({ minLeftWidth, minRightWidth, children, ...props }) => {\n  const [leftWidth, setLeftWidth] = React.useState(null);\n  const [totalWidth, setTotalWidth] = React.useState(null);\n  const separatorXPosition = React.useRef(null);\n\n  const splitPaneRef = React.createRef();\n\n  const onMouseDown = (e) => {\n    separatorXPosition.current = e.clientX;\n  };\n\n  const onMouseMove = (e) => {\n    if (!separatorXPosition.current) {\n      return;\n    }\n\n    const newLeftWidth = leftWidth + e.clientX - separatorXPosition.current;\n    if (\n      newLeftWidth > parseInt(minLeftWidth) &&\n      newLeftWidth < parseInt(totalWidth) - parseInt(minRightWidth)\n    ) {\n      separatorXPosition.current = e.clientX;\n      setLeftWidth(newLeftWidth);\n    }\n  };\n\n  const onMouseUp = () => {\n    separatorXPosition.current = null;\n  };\n\n  const onResize = (e) => {\n    setTotalWidth(splitPaneRef.current.clientWidth);\n  };\n\n  React.useEffect(() => {\n    if (!totalWidth) {\n      setTotalWidth(splitPaneRef.current.clientWidth);\n      return;\n    }\n  }, []);\n\n  React.useEffect(() => {\n    document.addEventListener(\"mousemove\", onMouseMove);\n    document.addEventListener(\"mouseup\", onMouseUp);\n    window.addEventListener(\"resize\", onResize);\n\n    return () => {\n      document.removeEventListener(\"mousemove\", onMouseMove);\n      document.removeEventListener(\"mouseup\", onMouseUp);\n      window.removeEventListener(\"resize\", onResize);\n    };\n  });\n\n  return (\n    <div {...props} ref={splitPaneRef}>\n      <splitPaneContext.Provider value={{ leftWidth, setLeftWidth }}>\n        <LeftSplit children={children[0]} />\n        <div\n          className=\"border-4 border-gray-200 cursor-move\"\n          onMouseDown={onMouseDown}\n        />\n        <RightSplit children={children[1]} />\n      </splitPaneContext.Provider>\n    </div>\n  );\n};\n\nconst LeftSplit = ({ children }) => {\n  const leftRef = React.createRef();\n  const { leftWidth, setLeftWidth } = React.useContext(splitPaneContext);\n  React.useEffect(() => {\n    if (!leftWidth) {\n      setLeftWidth(leftRef.current.clientWidth);\n      leftRef.current.style.flex = \"none\";\n      return;\n    }\n    leftRef.current.style.width = `${leftWidth}px`;\n  });\n  return (\n    <div className=\"w-1/2 border \" ref={leftRef}>\n      {children}\n    </div>\n  );\n};\n\nconst RightSplit = ({ children }) => {\n  return <div className=\"w-1/2\">{children} </div>;\n};\n\nexport default SplitPane;\n","/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/EditorAction.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/EditorNavBar.js",[],["180","181"],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/EditorContext.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/EditorContainer.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/NavBar.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/Logo.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/NavRightMenu.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/components/NavItem.js",[],"/Users/pawanbhatta/Desktop/DecProj/front_end/src/Use